<%- include('../layouts/header') %>
<div class="container mt-5">
  <h1>Available Vehicles</h1>
  <table class="table">
    <thead>
      <tr>
        <th>Name</th>
        <th>Brand</th>
        <th>Price</th>
        <th>Type</th>
        <th>Image</th>
      </tr>
    </thead>
    <tbody>
      <% vehicles.forEach(vehicle => { %>
        <tr>
          <td><%= vehicle.name %></td>
          <td><%= vehicle.brand %></td>
          <td><%= vehicle.price %></td>
          <td><%= vehicle.type %></td>
          <td><img src="<%= vehicle.image %>" alt="<%= vehicle.name %>" style="max-width: 100px;"></td>
        </tr>
      <% }) %>
    </tbody>
  </table>
</div>
<%- include('../layouts/footer') %>